  <xs:element name="listarPedidoPorLocal" type="tns:listarPedidoPorLocal" />
  <xs:element name="listarPedidoPorLocalCliente" type="tns:listarPedidoPorLocalCliente" />
  <xs:element name="listarPedidoPorLocalClienteResponse" type="tns:listarPedidoPorLocalClienteResponse" />
  <xs:element name="listarPedidoPorLocalResponse" type="tns:listarPedidoPorLocalResponse" />
  <xs:element name="obtenerDetallePedido" type="tns:obtenerDetallePedido" />
  <xs:element name="obtenerDetallePedidoList" type="tns:obtenerDetallePedidoList" />
  <xs:element name="obtenerDetallePedidoListResponse" type="tns:obtenerDetallePedidoListResponse" />
