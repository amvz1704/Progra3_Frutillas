    <xs:sequence>
      <xs:element minOccurs="0" name="orden" type="tns:ordenVentaDTO" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="actualizarOrdenResponse">
    <xs:sequence />
  </xs:complexType>
  <xs:complexType name="listarPedidoPorLocalCliente">
    <xs:sequence>
      <xs:element name="idLocal" type="xs:int" />
      <xs:element name="idCliente" type="xs:int" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="listarPedidoPorLocalClienteResponse">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="return" type="tns:ordenVentaDTO" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="obtenerPedidosPorCliente">
    <xs:sequence>
      <xs:element name="idCliente" type="xs:int" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="obtenerPedidosPorClienteResponse">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="return" type="tns:ordenVentaDTO" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="obtenerDetallePedido">
    <xs:sequence>
      <xs:element name="idOrdenVenta" type="xs:int" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="obtenerDetallePedidoResponse">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="return" type="tns:lineaOrdenDeVenta" />
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="estadoVenta">
    <xs:restriction base="xs:string">
      <xs:enumeration value="ENTREGADO" />
      <xs:enumeration value="PROCESO" />
      <xs:enumeration value="CAMBIO" />
      <xs:enumeration value="POR_ENTREGAR" />
      <xs:enumeration value="FALTA_PAGO" />
      <xs:enumeration value="CANCELADO" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="tipoEstado">
    <xs:restriction base="xs:string">
      <xs:enumeration value="DISPONIBLE" />
      <xs:enumeration value="AGOTADO" />
    </xs:restriction>
  </xs:simpleType>
</xs:schema>