  <xs:element name="obtenerPedidosPorEmpleadoResponse" type="tns:obtenerPedidosPorEmpleadoResponse" />
  <xs:complexType name="agregarOrden">
    <xs:sequence>
      <xs:element minOccurs="0" name="orden" type="tns:ordenVentaDTO" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ordenVentaDTO">
    <xs:sequence>
      <xs:element minOccurs="0" name="descripcion" type="xs:string" />
      <xs:element name="entregado" type="xs:boolean" />
      <xs:element minOccurs="0" name="estado" type="tns:estadoVenta" />
      <xs:element minOccurs="0" name="fechaStr" type="xs:string" />
      <xs:element minOccurs="0" name="horaStr" type="xs:string" />
      <xs:element name="idCliente" type="xs:int" />
      <xs:element name="idComprobante" type="xs:int" />
      <xs:element name="idEmpleado" type="xs:int" />
      <xs:element name="idLocal" type="xs:int" />
      <xs:element name="idOrdenVenta" type="xs:int" />
      <xs:element name="montoTotal" type="xs:double" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="agregarOrdenResponse">
    <xs:sequence />
  </xs:complexType>
  <xs:complexType name="listarPedidoPorLocal">
    <xs:sequence>
      <xs:element name="idLocal" type="xs:int" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="listarPedidoPorLocalResponse">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="return" type="tns:ordenVentaDTO" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="obtenerPedidoPorId">
    <xs:sequence>
      <xs:element name="idOrdenVenta" type="xs:int" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="obtenerPedidoPorIdResponse">
    <xs:sequence>
      <xs:element minOccurs="0" name="return" type="tns:ordenVentaDTO" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="generarOrdenConLineas">
    <xs:sequence>
      <xs:element minOccurs="0" name="orden" type="tns:ordenVentaDTO" />
      <xs:element minOccurs="0" maxOccurs="unbounded" name="lineas" type="tns:lineaOrdenDeVenta" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="lineaOrdenDeVenta">
    <xs:sequence>
      <xs:element name="cantidad" type="xs:int" />
      <xs:element name="idLineaVenta" type="xs:int" />
      <xs:element name="idOrdenVenta" type="xs:int" />
      <xs:element minOccurs="0" name="producto" type="tns:producto" />
      <xs:element name="subtotal" type="xs:double" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="producto">
    <xs:sequence>
      <xs:element minOccurs="0" name="codigoProd" type="xs:string" />
      <xs:element minOccurs="0" name="descripcion" type="xs:string" />
      <xs:element name="idProducto" type="xs:int" />
      <xs:element minOccurs="0" name="nombre" type="xs:string" />
      <xs:element name="precioUnitario" type="xs:double" />
      <xs:element name="stock" type="xs:int" />
      <xs:element name="stockMinimo" type="xs:int" />
      <xs:element minOccurs="0" name="tipoEstado" type="tns:tipoEstado" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="generarOrdenConLineasResponse">
    <xs:sequence>
      <xs:element name="return" type="xs:int" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="obtenerPedidosPorEmpleado">
    <xs:sequence>
