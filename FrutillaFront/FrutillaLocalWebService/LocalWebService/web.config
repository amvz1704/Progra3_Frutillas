<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.8.1"/>
    <httpRuntime targetFramework="4.8.1"/>
	  
	<authentication mode="Forms">
		<forms loginUrl="Login.aspx" timeout="30"/>
	</authentication>
	<authorization>
		<deny users="?"/>
	</authorization>
  </system.web>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="EmpleadoWSPortBinding" />
        <binding name="NotificacionWSPortBinding" />
        <binding name="UsuarioWSPortBinding" />
        <binding name="ClienteWSPortBinding" />
        <binding name="LocalWSPortBinding" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:8080/FrutillaWS/EmpleadoWS"
        binding="basicHttpBinding" bindingConfiguration="EmpleadoWSPortBinding"
        contract="EmpleadoWS.EmpleadoWS" name="EmpleadoWSPort" />
      <endpoint address="http://localhost:8080/FrutillaWS/NotificacionWS"
        binding="basicHttpBinding" bindingConfiguration="NotificacionWSPortBinding"
        contract="NotificionesWS.NotificacionWS" name="NotificacionWSPort" />
      <endpoint address="http://localhost:8080/FrutillaWS/UsuarioWS"
        binding="basicHttpBinding" bindingConfiguration="UsuarioWSPortBinding"
        contract="UsuarioWS.UsuarioWS" name="UsuarioWSPort" />
      <endpoint address="http://localhost:8080/FrutillaWS/ClienteWS"
        binding="basicHttpBinding" bindingConfiguration="ClienteWSPortBinding"
        contract="ClienteWS.ClienteWS" name="ClienteWSPort" />
      <endpoint address="http://localhost:8080/FrutillaWS/LocalWS"
        binding="basicHttpBinding" bindingConfiguration="LocalWSPortBinding"
        contract="LocalWS.LocalWS" name="LocalWSPort" />
    </client>
  </system.serviceModel>
  <system.codedom>
    <compilers>
      <compiler language="c#;cs;csharp" extension=".cs" warningLevel="4" compilerOptions="/langversion:default /nowarn:1659;1699;1701;612;618" type="Microsoft.CodeDom.Providers.DotNetCompilerPlatform.CSharpCodeProvider, Microsoft.CodeDom.Providers.DotNetCompilerPlatform, Version=4.1.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35"/>
      <compiler language="vb;vbs;visualbasic;vbscript" extension=".vb" warningLevel="4" compilerOptions="/langversion:default /nowarn:41008,40000,40008 /define:_MYTYPE=\&quot;Web\&quot; /optionInfer+" type="Microsoft.CodeDom.Providers.DotNetCompilerPlatform.VBCodeProvider, Microsoft.CodeDom.Providers.DotNetCompilerPlatform, Version=4.1.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35"/>
    </compilers>
  </system.codedom>
	<location path="SignIn.aspx">
		<system.web>
			<authorization>
				<allow users="*" />
			</authorization>
		</system.web>
	</location>
</configuration>